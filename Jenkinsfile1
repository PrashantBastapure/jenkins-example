pipeline {
	    agent any
	
	    stages {
	        stage ('Compile Stage') {
	
	            steps {
	                withMaven(maven: 'MAVEN_HOME') {
	                    sh 'mvn clean compile'
	                }
	            }
	        }
	
	        stage ('Testing Stage') {
	
	            steps {
	                withMaven(maven: 'MAVEN_HOME') {
	                    sh 'mvn test'
	                }
	            }
	        }
	
	       stage ('Dummy Stage1') {
	
	            steps {
	                echo "this is Dummmy stage"
	            }
	        }
		
		
		stage ('Dummy Stage2') {
	
	            steps {
	                echo "this is Dummmy stage2"
	            }
	        }
	
	        stage ('Deployment Stage') {
	            steps {
	                withMaven(maven: 'MAVEN_HOME') {
	                    //sh 'mvn deploy'
			    echo 'deploying'
	                }
	            }
	        }
	    }
	}

