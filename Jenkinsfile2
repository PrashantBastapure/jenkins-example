pipeline {

agent any

tools {
maven 'mymaven3.6.3'
}	

	stages {
				stage('test') {
								environment {
									mydockehub = credentials('DOKCER_HUB_PASSWORD')
								}
								
								
								
									steps {
										withMaven(maven: 'mymaven3.6.3') {
											sh 'mvn clean'
										}
										
										echo "mydockehub = ${env.mydockehub}"
									}
				}
	}
			
			
}
