pipeline {

agent any

tools {
mvn 'MAVEN_HOME'
}	

	stages {
				stage('test') {
								environment {
									mydockehub = credentials('DOKCER_HUB_PASSWORD')
								}
								
								echo 'mydockehub = ${env.mydockehub}'
								
									steps {
										sh 'mvn clean'
									}
				}
	}
			
			
}
