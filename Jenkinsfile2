pipeline {

agent any

tools {
maven 'MAVEN_HOME'
}	

	stages {
				stage('test') {
								environment {
									mydockehub = credentials('DOKCER_HUB_PASSWORD')
								}
								
								
								
									steps {
										sh 'maven clean'
										
										echo 'mydockehub = ${env.mydockehub}'
									}
				}
	}
			
			
}
